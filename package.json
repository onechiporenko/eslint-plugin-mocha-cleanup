{
  "name": "eslint-plugin-mocha-cleanup",
  "version": "1.6.2",
  "description": "Check mocha tests for empty titles and equal titles, huge number of assertions in the one test-block, invalid sinon usage etc. TDD and BDD syntaxes are supported",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "mocha",
    "cleanup"
  ],
  "repository": "https://github.com/onechiporenko/eslint-plugin-mocha-cleanup",
  "author": "onechiporenko",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/eslint . && mocha -R min tests/lib/rules",
    "test:fast": "mocha -R min tests/lib/rules --fast"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "~6.8.0",
    "jsonium": "^1.5.0",
    "mocha": ">=2.3.4"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
